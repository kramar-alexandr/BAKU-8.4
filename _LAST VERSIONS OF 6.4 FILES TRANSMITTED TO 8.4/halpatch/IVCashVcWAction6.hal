global
updating procedure ProceedGiftVoucherTouchScreenDClass()
begin
  Integer wn,mwn;
  record RcVc RepSpec;
  record IVVc prevIVr;
  record IVVc IVr;
  row IVVc IVrw;
  record IVCashVc prevIVCashr;
