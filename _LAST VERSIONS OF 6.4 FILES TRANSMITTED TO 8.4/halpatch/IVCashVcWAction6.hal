              if (IVCashPrintReceipt(mwn,IVCashr,prevIVCashr,printrownr,false,false,LMb.IncrementalReceiptPrinting,wnst)) then begin
              end;
            end;
          end;
          PutWindowString(mwn,"touchscreenitemlastrownr",MatRowCnt(IVCashr)-1);
        end;
        CloseWindow(wn);
        ReRunWindowDef(mwn);
    end;
  end;
