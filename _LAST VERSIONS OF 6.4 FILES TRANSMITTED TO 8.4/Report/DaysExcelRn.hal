                              sumbase = rmbpart - printedrmb;
                            end else begin
                              //sumbase = round(IVrw.Sum/IVr.Sum4*recivedmoneybase,SetRoundModeD(2));
                              sumbase = round(IVrw.Sum*recivedmoneybase/IPrw.RecVal,SetRoundModeD(2));
                              printedrmb = printedrmb + sumbase;
