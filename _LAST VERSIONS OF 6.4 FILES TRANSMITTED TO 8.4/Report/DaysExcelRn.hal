                              Oborot[tstr & "sum"] = Oborot[tstr & "sum"] + sumoborot;
                            end;
                            j = rwcnt2;
                          end;
                          if (IVrw.stp==kInvoiceRowTypeNormal) then begin
                            artcnt = artcnt + 1;
