								if(IVrw.stp==kInvoiceRowTypeLoyaltyPointsPayment)then begin
									if(IVr.FrRate!=0)then begin
										CUDebSum = CUDebSum + IVrw.Sum*IVr.ToRateB1/IVr.FrRate;
									end else begin
										CUDebSum = CUDebSum + IVrw.Sum;
									end;
								end; 
