													dif = dif - IPrw.InvVal;
												end;
											end;
										end; 
									end;
								end;
							end;
							resetloop(IPrsr1);
							
							if(dif!=0)then begin
								/*startformat(15);
									outstring(0,"DblIVVc",IVr.SerNr,false);
									Outstring(50,0,IVr.CurncyCode,false);
									Outstring(100,0,dif,false);
								endformat;*/
								
