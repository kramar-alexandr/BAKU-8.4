  Boolean res;
  Integer nwn,cnt,i;
	record BigPUVc BPUr;
	row BigPUVc BPUrw;
	record PUVc PUr;
  GetWindowRecord(wn,BPUr);
	res = true;
	if(BPUr.OKFlag!=0) then begin res = false; goto LBPUDClassActiveEditField; end;
	if(BPUr.ApprovedCost6 and fieldname=="Tax15PercentSum") then begin res = false; end; //Edit **********************************************Vas-P 22/06/2021
