procedure slIVHAL(Integer wn)
BEGIN
  record IVVc IVr;
  record DIVc DIr;
  Boolean found;
  string 255 ckey,t2,tstr;

  ClearStringList(wn);
  found = true;
  ckey = "DICode:" & GetWindowSubset(wn);
  while (LoopKey(ckey,DIr,1,found)) begin
    tstr = DIr.Name & " ";
    tstr = tstr & DIr.colnr;
    SetListString(wn,len(tstr),tstr,false);
  end;
  PutWindowRecord(wn,IVr);
  RETURN;
END;          

