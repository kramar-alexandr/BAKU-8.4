procedure IDCreateSDFromOR(record ORVc ORp, string comment, string obj)
begin
  record SDVc SDr;
  record LocationVc Locr;
  row ORVc ORrw; 
  row SDVc SDrw;
  integer nwn,rwcnt,i,j;
  boolean artblankf;
  string 255 tstr;
  
  rwcnt = MatRowCnt(ORp);
  artblankf = false;
  for (i=0;i<rwcnt;i=i+1) begin
    MatRowGet(ORp,i,ORrw);
    if (ORrw.stp==kInvoiceRowTypeNormal) and blank(ORrw.ArtCode) then begin
      artblankf = true;
      i = rwcnt;
    end;
  end;
  if (artblankf) then begin
    messagebox(35141,"");
  end else begin
    RecordNew(SDr);
    SDr.Location = ORp.Location;
    if nonblank(SDr.Location) then begin
      Locr.Code = SDr.Location;
      ReadFirstMain(Locr,1,true);
      SDr.Objects = AddObjectToObjectList(SDr.Objects,Locr.Objects);
    end;
    SDr.Objects = AddObjectToObjectList(SDr.Objects,obj);
    SDr.Comment = comment;
    SDr.RefStr = ORp.CustOrdNr;
    for (i=0;i<rwcnt;i=i+1) begin
      MatRowGet(ORp,i,ORrw);
      if (ORrw.stp==kInvoiceRowTypeNormal) then begin
        ClearRow(SDr,SDrw,1);
        SDrw.ArtCode = ORrw.ArtCode;
        SDrw.SerialNr = ORrw.SerialNr;
        SDrw.Qty = ORrw.Quant;
        MatRowPut(SDr,j,SDrw);
        j = j + 1;
        SDVc_PasteArtCode(SDr,i,1,tstr,tstr);
      end;
    end;
		if(obj=="E125") then begin
			SDr.CostAcc = "44/12";
		end;
		if(obj=="E127") then begin
			SDr.CostAcc = "44/01";
		end;
		if(obj=="E1271") then begin
			SDr.CostAcc = "44/01";
		end;
    nwn = OpenWindow("SDDClass",1,0,"","",SDr);
  end;
  return;
end;

