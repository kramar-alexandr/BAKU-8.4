global
procedure IDSaleContractBtnClick()
begin
  record ORVc ORr;
  integer wn;
	boolean testf;
	record SellerDataBlock SDb;
	record UserVc Usr;
	record CUVc CUr;
	string 255 CustFullName, CustPoss, SMName;
	
	
	
	blockload(SDb);
	
  wn = CurWindow;
  
  if(windowstate(wn)==Rs_normal)then begin
  
		DeselectWindow(wn,true);
		GetWindowRecord(wn,ORr);
	
	
	
		CUr.Code = ORr.CustCode;
		if(ReadFirstMain(CUr,1,true))then begin
			CustFullName = CUr.Name;
		end;
	
		Usr.Code = ORr.SalesMan;
		if(ReadFirstMain(Usr,1,true))then begin
			CustPoss = Usr.Job;
			SMName = Usr.Name;
		end;
	
	
	
		if (left(ORr.OrderClass,2)=="IP") or (left(ORr.OrderClass,2)=="ID") or (left(ORr.OrderClass,2)=="IS") then begin
			testf = true;
