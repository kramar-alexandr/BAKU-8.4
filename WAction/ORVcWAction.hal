function Integer ORLClassRowColor(record ORVc ORr)
begin
  Integer res,i,mtrw,sum1,sum2;
	row ORVc ORrw;
	if(currentcompany==29)then begin
		if(ORr.Closed==0 and ORr.OKFlag==0)then begin
			if(ORr.ECORSuccOK==1)then begin
				res = kRowcolorGreen;
			end else begin
				if(ORr.ECORLocCompl==1 and ORr.ECSHOK!=1)then begin
					res = kRowcolorYellow;
				end else begin
					if(ORr.OrderConfirmed==1 or ORr.ECSHOK==1)then begin
					end else begin
						res = kRowcolorRed;
					end;
				end;
			end;
		end else begin
			if(ORr.Closed==1 and ORr.OKFlag==0 and ORr.ECORSuccOK==0)then begin
				res = kRowcolorRed;
			end else begin
				res = kRowColorNone;
			end;
		end;
  end;
  ORLClassRowColor = res;
  return;
end;  // _-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_- Anton Preisinger15:40 12.12.2018

