function Boolean ORDClassLocationEFAfter(Integer wn,Integer rownr,Boolean changedf)
begin
  record ORVc ORr;
	row ORVc ORrw;
	val CISqty;
	boolean TrHs;
	integer mtrwcnt,i;
	record ItemStatusVc ISr;
	record ClientItemStatusVc CISr;
	
  
  if (changedf) then begin
    GetWindowRecord(wn,ORr);
    ORVc_PasteLocation(ORr,rownr);
		if(rownr==-1 and currentcompany==18) then begin
			mtrwcnt = matrowcnt(ORr);
			for(i=0;i<mtrwcnt;i=i+1) begin
				matrowget(ORr,i,ORrw);
				CISqty = 0;
				TrHs = true;
				while (loopmain(CISr,1,TrHs)) begin
					if(CISr.ItemCode!=ORrw.ArtCode)then begin TrHs = false; end;
					if(CISr.OrderNr!=ORr.SerNr and ORr.OrderClass=="CLIEN" and TrHs)then begin
						CISqty = CISqty + CISr.Quant;
					end;
					if((ORr.OrderClass=="STOCK" or ORr.OrderClass=="S_IDE") and TrHs)then begin
						CISqty = CISqty + CISr.Quant;
					end;
				end;
				resetloop(CISr);
				ISr.Code = ORrw.ArtCode;
				if(blank(ORr.Location)) then begin 
					ISr.Location = ";;;"; 
				end else begin
					ISr.Location = ORr.Location; 
				end;
				if(readfirstmain(ISr,2,true)) then begin
					ORrw.IDAmount = ISr.Instock - ISr.RsrvQty - CISqty;
				end;
				matrowput(ORr,i,ORrw);
			end;	
		end;	
    PutWindowRecord(wn,ORr);
  end;
  ORDClassLocationEFAfter = true;
  return;
end;

