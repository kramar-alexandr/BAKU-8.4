global procedure ItemSaldoGlobalRn(record RcVc RepSpec)
begin
	integer curcomp;
	
	curcomp = currentcompany;
	
	if(curcomp!=18)then begin
		setcompany(18,false);
	end;
	
	StartReportNoHeaderJob("Оборотно-сальдовая ведомость по товарам Глобальная");
	
	
	endjob;
	
	if(curcomp!=18)then begin
		setcompany(curcomp,false);
	end;
	
return;
end;
