  foundf = false;
  iteminf = false;
  rwcnt = MatRowCnt(SD2r);
  for (i=0;i<rwcnt;i=i+1) begin
  	MatRowGet(SD2r,i,SD2rw);
    if (item==SD2rw.ArtCode and location==SD2rw.Location) then begin
      pos = i;
      coefv = SD2rw.Qty;
      foundf = true;
      iteminf = true;
      i = rwcnt;
      SD2rw.BasePrice = SD2rw.BasePrice + 1;
    end;
