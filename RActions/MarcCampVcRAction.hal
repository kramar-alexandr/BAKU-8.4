					d1.day = stringtoint(mid(MyJSGet(jsresponse,"enddate"),8,2));
					d1.month = stringtoint(mid(MyJSGet(jsresponse,"enddate"),5,2));
					LCr.ExpiryDate = d1;
					LCr.Closed = 1;
				recordstore(LCr,true);
			end;
		end else begin
			weboutstring("{\"error\":\"Card already created\"}");
		end;			
	end;
return;
end;

