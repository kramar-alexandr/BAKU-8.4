					logtext(0,"Create Loyalty card with blank customer");
					LCr.SerNr = uppercase(MyJSGet(jsresponse,"HansaId"));
					LCr.CustCode = MyJSGet(jsresponse,"Contact");
					LCr.LCMLevel = MyJSGet(jsresponse,"LoyaltyProgramId");
