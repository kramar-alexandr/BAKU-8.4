											IVr.OKFlag = 0;
											if(recordupdate(OldIVr,IVr,true)==0)then begin
												recorddelete(IVr);
											end;
										end else begin
											recorddelete(IVr);
										end;
									end;
								end;
							end;
						end;
					end;
					if(stat==USetStr(36214))then begin  // 36214 - подтвержден
						RecordNew(Actr);
						ActVcRecordDefClient(Actr);
						if (MakeActFromIV(IVr,Actr)) then begin 	
							RecordCopy(oldActr,Actr);
							Actr.Comment = "Создан заказ на сборку " & ORr.SerNr;
							cscnt = matrowcnt(APCDSb);
							for(c=0;c<cscnt;c=c+1)begin
								matrowget(APCDSb,c,APCDSrw);
								if(APCDSrw.LocCode==ORr.Location)then begin
									Actr.MainPersons = APCDSrw.CollectorName;
								end;
							end;
							if(blank(Actr.MainPersons))then begin
								Actr.MainPersons = "SA";
							end;
							if(blank(Actr.TransDate))then begin
								Actr.TransDate = CurrentDate;
							end;
							Actr.TodoFlag = 1;
							Actr.CalTimeFlag = kCalTimeFlagTime;
							Actr.AlarmType = 1;
							Actr.SymbNr = 3;
							if(RecordUpdate(oldActr,Actr,true)==0)then begin 
								CreateRecordLink(Actr,Currentcompany,ORr,Currentcompany);
								CreateRecordLink(ORr,Currentcompany,Actr,Currentcompany);
							end;
						end;
					end;
					if(rsrv==false)then begin
						WebOutString("{\"result\":\"ok\",\"invoiceId\":" & IVr.SerNr & "}");
					end else begin
						WebOutString("\"result\":\"ok\",\"invoiceId\":" & IVr.SerNr & "}");
					end;
				end;
			end;
		end;
	end;
return;
end;