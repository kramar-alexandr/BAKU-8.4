global webpublic updating procedure WebGetSendClientPrices()
begin
	string 100 artcode,brand,client,invid,code,warning,errstr;
	record ORVc ORr;
	record CUVc CUr;
	row ORVc ORrw;
	area getpost;
	json jobj;
	integer i,j,rwcnt,compno;
	boolean isfirst;
	vector string 150 vcode;
	array string 255 aCodes;
	string 255 filename;
	
	// setcompany(1,false);
	
	logtext(0,"WebGetSendClientPrices");
	
	WebSetContentType("application/json");		
	
	webgetpostdata(getpost);
	jobj = parsejsonarea(getpost);
	client = JSONGet(jobj,"client");
	compno = StringToInt(JSONGet(jobj,"compno"));
	
	filename = "ClientPrices/ClientPrices" & datetostring(currentdate,"YYYYMMDD") & TimeToString(currenttime,"HHMMSS",false) & ".txt";
	writeareatofile(getpost,filename,0);
	
	if(blank(client))then begin
		client = "NONAME";
	end;
	if(compno>0) then begin
	  setcompany(compno,false);
		for(i=0;i<JSONCountChildren(jobj,"basket_items");i=i+1) begin
			code = JSONGet(jobj,"basket_items[0]/["  & i & "]");
			vcode[code] = DeleteBackSlash(code);
		end;
		weboutstring("{\"result\":");
		weboutstring("{" & chr(13) & chr(10));
		
		RecordNew(ORr);
		CUr.Code = DeleteBackSlash(client);
		if(readfirstmain(CUr,1,true))then begin
			ORr.PriceList = CUr.PLCode;
		end else begin
			client = "NONAME";
			ORr.PriceList = "RRP";
		end;
		ORr.CustCode = client;
		if (PasteCustInOrder(ORr,ORr.CustCode,"",warning,errstr)) then begin
	  end;
		
		weboutstring("\"client\":{" & chr(13) & chr(10));
		weboutstring("\"custcode\":\"" & ORr.CustCode & "\"," & chr(13) & chr(10));
		weboutstring("\"compno\":\"" & compno & "\"" & chr(13) & chr(10));
		weboutstring("}," & chr(13) & chr(10));
		weboutstring("\"basket_items\":{" & chr(13) & chr(10));
		
		ORr.PayDeal = "O";
		GetVectorTags(vcode,aCodes);
		for(j=0;j<=aCodes.length;j=j+1)begin
			rwcnt = MatRowCnt(ORr);
			if(nonblank(vcode[aCodes[j]]))then begin
				ORrw.ArtCode = aCodes[j];
				MatRowPut(ORr,rwcnt,ORrw);
				ORVc_PasteArtCode(ORr,rwcnt,warning,warning,false);
				MatRowGet(ORr,rwcnt,ORrw);
				if(isfirst)then begin
					WebOutString(",");
				end;
				isfirst = true;
				weboutstring("\"" & ORrw.ArtCode & "\":{" & chr(13) & chr(10));
				weboutstring("\"price\":\"" & ORrw.Price & "\"," & chr(13) & chr(10));
				weboutstring("\"rebate\":\"" & ORrw.vRebate & "\"" & chr(13) & chr(10));
				weboutstring("}" & chr(13) & chr(10));
			end;
		end;
		weboutstring("}" & chr(13) & chr(10));
		weboutstring("}" & chr(13) & chr(10));
		weboutstring("}");
		
	end else begin
		weboutstring("{\"result\":\"compno cant be 0 or empty\"}");
	end;
return;
end;



