window MATVARINSClass:2
  wtag("language","RUS"), wtag("product","*"), wtag("productcode","*"), wtag("device","computer")
begin
  array string 255 axlabels,aylabels;
  integer pos,xpos,ypos,yposmax,ylabellen,starth;
  real h,v,matheightbase;
  string 255 label;
  
  WindowBegin("Ввод матрицы вариантов товара",MATVARINSClass,CGother,VarMatrxEntryView);
  DynamicWindow;
  xpos = CntVarietyCode(1,axlabels);
  ypos = CntVarietyCode(2,aylabels);
  yposmax = VarietyMatMax(ypos);
  for (pos=1;pos<=ypos;pos=pos+1) begin
    label = aylabels[pos];
    if (ylabellen<len(label)) then begin
      ylabellen = len(label);
    end;
  end;
  starth = 7*ylabellen;
  if (starth<70) then begin starth = 70; end;
  SetWRect(80,80,200+(xpos*80)+(starth-70),125+(yposmax*17)+35);
  UseView(VarMatrxEntryVc);
  h = 10; v = -40;
  if (WindowStyle("MasterWClass")==kWindowStyleWindows8Desktop) then begin
    matheightbase = 66;
  end else begin
    matheightbase = 63;
  end;
  if (GuiType==kGuiCocoa) then begin
    MatrixBegin(starth,50,-32,-10,VarMatrxEntryRowType,ypos);
  end else begin
    MatrixBegin(starth,50,-32,matheightbase+ypos*17,VarMatrxEntryRowType,ypos);
  end;
  for (pos=1;pos<=xpos;pos=pos+1) begin
    label = axlabels[pos];
    switch (pos) begin
      case 1: MatCol(1,v+=80,label,0,A,false,0);
      case 2: MatCol(1,v+=80,label,0,B,false,0);
      case 3: MatCol(1,v+=80,label,0,C,false,0);
