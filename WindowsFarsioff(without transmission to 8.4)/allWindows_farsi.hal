        CheckBox(h,v+=16,0,"? ?   ?? ? ",flags[11]);     
      end else begin
        CheckBox(h,v+=16,0,"? ? ?   ?? ? ",flags[10]);  
      end;
    end;
    StaticText(30,v+=50,"? ?   ?  ? ?  ? ? ",false);
  end;
  EndWindow;
end;

