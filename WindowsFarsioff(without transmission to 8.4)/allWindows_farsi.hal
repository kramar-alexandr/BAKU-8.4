    CheckBox(h,v+=16,0,"   ",flags[14]);
    CheckBox(h,v+=16,0,"? ? ",flags[15]);
    ButtonFrame(h=220,v=vm+26,170,4," ? ? ");
    RadioButton(h,v,0,0,"?? ??",SimVerf);
    RadioButton(h,v+=16,1,0," ",SimVerf);
    RadioButton(h,v+=16,2,0," ??  1 ",SimVerf);
    RadioButton(h,v+=16,3,0," ??  1  2 ",SimVerf);
  end else begin
    PeriodField(h=140,v=6," ");
    EditFieldTL(h,v+=20,150,"",M4UStr,Normal,20,AccStr,false,AccSClass);
    if (HasObjects) then begin
      EditFieldTL(h,v+=20,150,"?",M4UStr,Normal,60 ,ObjStr,false,ObjSClass);
    end;
    EditFieldTL(h,v+=20,82,"   ? ",M4Date,Normal,0,d2,false,0);
    if (HasLocalization("POL,PRT")) or (IsEnterprise) then begin
      EditFieldTL(h,v+=20,50,"?  ",M4Long,Normal,0 ,FirstVer,false,0);
    end;
    if (HasObjects) then begin
      EditFieldTL(h,v+=20,50," ? ",M4UStr,Normal,5 ,ObjType,false,OTSClass);
      EditFieldTL(h,v+=vs,80," ? ",M4UStr,Normal,60,f3,false,ObjSClass);
    end;
    if (HasLanguages) then begin
      EditFieldTL(h,v+=vs,50," ",M4UStr,Normal,5 ,Language,false,LangSClass);
    end;
    if (HasMultiCurrency or HasBase2) then begin
      ButtonFrame(h,v+=40,170,2,"   ");
      RadioButton(h,v,0,0," ?? 1",basecurncy);
      RadioButton(h,v+=16,1,0,"??  2",basecurncy);
    end;
    if (HasLocalization("POL,PRT")) then begin
      ButtonFrame(h,v+=40,170,2," ");
      RadioButton(h,v,0,0," ",ArtMode);
      RadioButton(h,v+=16,1,0,"  ? ? ",ArtMode);
