    EditFieldTL(h,v+=20,50,"?  ",M4Long,Normal,0 ,FirstVer,false,0);
    EditFieldTL(h,v+=20,50," ? ",M4UStr,Normal,5 ,ObjType,false,OTSClass);
    EditFieldTL(h,v+=vs,80," ? ",M4UStr,Normal,60,f3,false,ObjSClass);
    ButtonFrame(h=20,vm=(v+=95),170,2,"   ");
    RadioButton(h,v,0,0," ?? 1",basecurncy);
    RadioButton(h,v+=16,1,0,"??  2",basecurncy);
    ButtonFrame(h=220,v=vm,170,2," ");
    RadioButton(h,v,0,0," ",ArtMode);
    RadioButton(h,v+=16,1,0,"  ? ? ",ArtMode);
    StaticText(20,v+=50,"? ?   ?  ? ?  ? ? ",false);
    Tile(2,280,false,"2",Language);
    v = 6; h=140;
    EditFieldTL(h,v,50," ",M4UStr,Normal,5 ,Language,false,LangSClass);
    CheckBox(h=20,vm=(v+=20),0," ? ",flags[1]);
    CheckBox(h,v+=16,0,"?  ",flags[4]);
    CheckBox(h,v+=16,0," ? ",flags[8]);
    CheckBox(h,v+=16,0,"? ? ?   ?? ? ",flags[10]);  
    CheckBox(h,v+=16,0,"? ?   ?? ? ",flags[11]);
    CheckBox(h,v+=16,0,"?? ? ",flags[12]);
