    if (HasLocalization("PRT")) then begin
      ButtonFrame(h,v+=40,170,4," ? ? ");
      RadioButton(h,v,0,0,"?? ??",SimVerf);
      RadioButton(h,v+=16,1,0," ",SimVerf);
      RadioButton(h,v+=16,2,0," ??  1 ",SimVerf);
      RadioButton(h,v+=16,3,0," ??  1  2 ",SimVerf);
    end;
    if (HasLocalization("POL,PRT")) then begin
      CheckBox(h,v+=24,0," ? ",flags[8]);
    end;
    if (HasLocalization("POL,PRT")) then begin
      CheckBox(h,vm=(v+=16),0," ? ",flags[1]);
