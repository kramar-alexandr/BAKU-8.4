function string 255 prepareStr(string str)
begin
	integer i,lenth;
	string 255 res;
	
	res = "";
	lenth = len(str);
	For(i=0;i<lenth;i=i+1) begin
		switch(mid(str,i,1))begin
			case"&":	res = res & "&amp;";
			case"<":	res = res & "&lt;";
			case">":	res = res & "&gt;";
			otherwise
			res = res & mid(str,i,1);
		end;
	end; 
	prepareStr = res;
	
return;
end;

