global 
webpublic procedure WebRestoreBrndsMn()
begin
	record GlobalItemVc GIr, logGIr;
	record BTRxBrandVc BTRxBrandr, oldBrandr;
	boolean TrHs, TrHs2;
	string 255 OldCode, NewCode;
	vector string 255 newbrand;
	vector boolean newbrandf;
	
	
	setcompany(1,false);
	
	GIr.Code = "NOVAS";
	TrHs = true;
	while (loopMain(GIr,1,TrHs)) begin
		if (left(GIr.Code,5)!="NOVAS") then begin TrHs = false; end;
		if (TrHs) then begin
			BTRxBrandr.Name = GIr.Brand;
			if (!ReadFirstKey("Name",BTRxBrandr,1,true) and !newbrandf[GIr.Brand]) then begin
				if(blank(OldCode))then begin 
					oldBrandr.Code = "MXBR9999";
					TrHs2 = true;
					while (LoopBackKey("Code",oldBrandr,1,TrHs2)) begin
						if (left(oldBrandr.Code,4)!="MXBR") then begin TrHs2 = false; end;
						if (TrHs2) then begin
							OldCode = oldBrandr.Code;
							TrHs2 = false;
						end;
					end;
					ResetLoop(oldBrandr);
					if(blank(OldCode))then begin OldCode = "MXBR0000"; end;
				end;
				NextCode(OldCode,NewCode);
				OldCode = NewCode;
				logtext(0,BTRxBrandr.Code);
				BTRxBrandr.Code = NewCode;
				BTRxBrandr.Name = GIr.Brand;
				if (nonblank(GIr.Brand)) then begin
					newbrandf[BTRxBrandr.Name] = true;
					newbrand[BTRxBrandr.Name] = BTRxBrandr.Code;
					queued.SaveExtBr(BTRxBrandr);
					millisleep(50);
				end;
			end else begin
				if (BTRxBrandr.Code!=GIr.Brand) then begin
					newbrandf[BTRxBrandr.Name] = true;
					newbrand[BTRxBrandr.Name] = BTRxBrandr.Code;
				end;
			end;
		end;
	end;
	resetloop(GIr);


	GIr.Code = "NOVAS";
	TrHs = true;
	while (loopMain(GIr,1,TrHs)) begin
		if (left(GIr.Code,5)!="NOVAS") then begin TrHs = false; end;
		if (TrHs) then begin
			if (nonblank(newbrand[GIr.Brand])) then begin
				if (GIr.BPIBrand!=newbrand[GIr.Brand]) then begin
					RecordCopy (logGIr,GIr);
					GIr.BPIBrand = newbrand[GIr.Brand];
					queued.SaveExtGirBr(GIr,logGIr);
					millisleep(50);
				end;
			end;
		end;
	end;



return
end;









