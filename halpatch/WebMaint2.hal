global updating procedure DelBrInFrQD(record INVc INr)
begin
	ReadFirstMain(INr,1,true);
	if (left(INr.Name,1)=="\"" and right(INr.Name,1)!="\"") then begin
		INr.Name = right(INr.Name,len(INr.Name)-1);
		recordstore(INr,true);
	end;
	return;
end;



global webpublic procedure WebDelBrInFr()
begin
	integer i;
	record INVc INr;
	
	for (i=0;i<35;i=i+1) begin
		SetCompany(i+1,false);
		INr.Code = "";
		while (loopmain(INr,1,true)) begin
			if (left(INr.Name,1)=="\"" and right(INr.Name,1)!="\"") then begin
				queued.DelBrInFrQD(INr);
				MilliSleep(50);
			end;
		end;
		resetloop(INr);
	end;

	return;
end;


global webpublic updating procedure WebFixBPIBrandGIr()
begin
	integer i;
	record INVc INr;
	record GlobalItemVc GIr;
	boolean chna;
	
	for (i=0;i<35;i=i+1) begin
		SetCompany(i+1,false);
		INr.Code = "";
		if(i+1!=18 and i+1!=29)then begin
      while (loopmain(INr,1,true)) begin
        if(blank(INr.GlobalArtCode) and nonblank(INr.BPIBrand))then begin
            chna = false;
            GIr.Code = INr.BPIBrand & "_" & INr.Code;
            if(readfirstmain(Gir,1,true))then begin
              INr.GlobalArtCode = GIr.Code;
              chna = true;  
            end;
            if(chna)then begin
              recordstore(INr,true);
            end;
            logtext(0,i+1 & " wrongcode " & INr.Code & " GI " & INr.GlobalArtCode);
        end;
      
      end;
      resetloop(INr);
		end;
	end;

	return;
end;








global webpublic updating procedure WebSynItemDupMn()
begin
	integer i;
	record ItemDuplicatesVc IDr;
	array string 255 OrCode, DupCode, OBrandCode, DBrandCode;
	record ConsItemVc CIr;


	SetCompany(18,false);
	i = 0;
	while (loopmain(IDr,1,true)) begin
		CIr.Code = IDr.OrigCode;
		if (ReadFirstMain(CIr,1,true)) then begin
			OrCode[i] = CIr.LocCode;
		end;
		CIr.Code = IDr.DupCode;
		if (ReadFirstMain(CIr,1,true)) then begin
			DupCode[i] = CIr.LocCode;
		end;
		OBrandCode[i] = IDr.OrigBrandCode;
		DBrandCode[i] = IDr.DupBrandCode;
		i=i+1;
	end;
	SetCompany(1,false);
	for (i=0;i<OrCode.length;i=i+1) begin
		IDr.OrigCode = OrCode[i];
