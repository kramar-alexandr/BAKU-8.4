  record GlobalItemVc GIr,LogGIr;
  row GlobalItemVc GIrw;
  integer i,rwcnt;
  string 100 timestamptstr,timestamptstr1;
  boolean storef, TrHs;
  
  logtext(0,"WebFixGlobalItemTimeStamp start");
  GIr.Code = "ECMXA";
	TrHs = true;
  while(loopmain(GIr,1,TrHs))begin
		if (left(GIr.Code,5)!="ECMXA") then begin TrHs = false; end;
		if (TrHs) then begin
			storef = false;
			recordcopy(LogGIr,GIr);
			timestamptstr = datetostring(GIrw.CurrentDate,"YYMMDD") & TimeToString(GIrw.CurrentTime,"HHMM",false);
			GIr.TimeStamp = stringtolongint(timestamptstr);
			GIWasUpdated(GIr,LogGIr," GIWasUpdatedlog 101");
			recordstore(GIr,true);
			logtext(0,"WebFixGlobalItemTimeStamp " & GIr.Code);
		end;
  end;
  
  logtext(0,"WebFixGlobalItemTimeStamp end");
  
return;
end;

