global webpublic procedure WebGetGlobTypeNamest()
begin
	record GlobalItemVc GIr;
	record BtrxTypeVc Typer;
	record BtrxFirstLevelCatVc BtrxFirstLevelCatr;
	record BtrxSecondLevelCatVc BtrxSecondLevelCatr;
	record BtrxThirdLevelCatVc BtrxThirdLevelCatr;
	record BtrxFourthLevelCatVc BtrxFourthLevelCatr;
	vector string 255 vBtrxFirstLevelCatr, vBtrxSecondLevelCatr, vBtrxThirdLevelCatr, vBtrxFourthLevelCatr;
	string 255 WEBSSectStr, tmp;
	integer pos;
	
	weboutstring("<table>");
	
	BtrxFirstLevelCatr.Code = "";
	while (loopMain(BtrxFirstLevelCatr,1,true)) begin
		vBtrxFirstLevelCatr[BtrxFirstLevelCatr.Code] = BtrxFirstLevelCatr.Name;
	end;
	
	BtrxSecondLevelCatr.Code = "";
	while (loopMain(BtrxSecondLevelCatr,1,true)) begin
		vBtrxSecondLevelCatr[BtrxSecondLevelCatr.Code] = BtrxSecondLevelCatr.Name;
	end;
	
	BtrxThirdLevelCatr.Code = "";
	while (loopMain(BtrxThirdLevelCatr,1,true)) begin
		vBtrxThirdLevelCatr[BtrxThirdLevelCatr.Code] = BtrxThirdLevelCatr.Name;
	end;
	
	BtrxFourthLevelCatr.Code = "";
	while (loopMain(BtrxFourthLevelCatr,1,true)) begin
		vBtrxFourthLevelCatr[BtrxFourthLevelCatr.Code] = BtrxFourthLevelCatr.Name;
	end;
	
	
	WEBSSectStr = "";
	pos = 0;
	ExtractObj(GIr.BTRxGiftClass,pos,tmp);
	while (nonblank(tmp)) begin
		if (nonblank(vBtrxThirdLevelCatr[tmp])) then begin
			if (nonblank(WEBSSectStr)) then begin WEBSSectStr = WEBSSectStr & ","; end;
			WEBSSectStr = WEBSSectStr & vBtrxThirdLevelCatr[tmp];
		end;
		if (nonblank(vBtrxFourthLevelCatr[tmp])) then begin
			if (nonblank(WEBSSectStr)) then begin WEBSSectStr = WEBSSectStr & ","; end;
			WEBSSectStr = WEBSSectStr & vBtrxFourthLevelCatr[tmp];
		end;
		ExtractObj(GIr.BTRxGiftClass,pos,tmp);
	end;
		
	weboutstring("<tr>");
		weboutstring("<td>" & "Code" & "</td>");
		weboutstring("<td>" & "Brand" & "</td>");
		weboutstring("<td>" & "First Level Cat." & "</td>");
		weboutstring("<td>" & "Second Level Cat." & "</td>");
		weboutstring("<td>" & "Third Level Cat." & "</td>");
		weboutstring("<td>" & "Fourth Level Cat." & "</td>");
		weboutstring("<td>" & "Additiional Cat." & "</td>");
	weboutstring("</tr>");

	GIr.Code = "";
	while (loopmain(GIr,1,true)) begin
			
		WEBSSectStr = "";
		pos = 0;
		ExtractObj(GIr.BTRxGiftClass,pos,tmp);
		while (nonblank(tmp)) begin
			if (nonblank(vBtrxThirdLevelCatr[tmp])) then begin
				if (nonblank(WEBSSectStr)) then begin WEBSSectStr = WEBSSectStr & ","; end;
				WEBSSectStr = WEBSSectStr & vBtrxThirdLevelCatr[tmp];
			end;
			if (nonblank(vBtrxFourthLevelCatr[tmp])) then begin
				if (nonblank(WEBSSectStr)) then begin WEBSSectStr = WEBSSectStr & ","; end;
				WEBSSectStr = WEBSSectStr & vBtrxFourthLevelCatr[tmp];
			end;
			ExtractObj(GIr.BTRxGiftClass,pos,tmp);
		end;
		weboutstring("<tr>");
			weboutstring("<td>" & GIr.Code & "</td>");
			weboutstring("<td>" & GIr.Brand & "</td>");
			weboutstring("<td>" & vBtrxFirstLevelCatr[GIr.BTRxFirstLevCat] & "</td>");
			weboutstring("<td>" & vBtrxSecondLevelCatr[GIr.BTRxSecondLevCat] & "</td>");
			weboutstring("<td>" & vBtrxThirdLevelCatr[GIr.BTRxThirdLevCat] & "</td>");
			weboutstring("<td>" & vBtrxFourthLevelCatr[GIr.BTRxFourthLevCat] & "</td>");
