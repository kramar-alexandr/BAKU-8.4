                retrw = AddPURowToRetPU(RetPUp,pui,PUr.SerNr,POrw,PUrw,CAb,ARb);
                res = 0;
                rem = rem - PUrw.Quant;
                if (rem<=0) then begin 
                  found = false; 
                  found2 = false;
                end else begin
                  found2 = true;
                end;
              end;
            end;
          end;
        end;
