            purwcnt = MatRowCnt(PUr);
            for (pui=0;pui<purwcnt;pui=pui+1) begin
              MatRowGet(PUr,pui,PUrw);
              if (PUrw.OrdRow==i) and (PUrw.PONr==POr.SerNr) then begin
                retrw = AddPURowToRetPU(RetPUp,pui,PUr.SerNr,POrw,PUrw,CAb,ARb);
                res = 0;
                rem = rem - PUrw.Quant;
                if (rem<=0) then begin 
                  found = false; 
                  found2 = false;
                end else begin
                  found2 = true;
                end;
              end;
