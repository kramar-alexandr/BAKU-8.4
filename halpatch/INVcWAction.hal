function Boolean INDClassCategoryEFAfter(Integer wn,Integer changed)
begin
  Boolean res;
  record INVc INr, IN2r;
	Array string 255 astr;
  Integer i,acnt,rwcnt,strlen,j,y;
	record IdINSNOneBlock IINSNOb;
	row IdINSNOneBlock IINSNOrw;
	string 255 SNOneNumber;
	val SNMiddleval;
	boolean Catef;
	
	
	
  Catef = false;
  INCatSClassArrayOnOpen(astr,acnt);
  
  if (changed!=0) then begin
		
    res = true;
    DeselectWindow(wn,false);
    GetWindowRecord(wn,INr);
    for (i=0;i<acnt;i=i+1) begin
			if(left(astr[i],4)==INr.Category)then begin
				INr.CategoryName = mid(astr[i],15,len(astr[i]));
				blockload(IINSNOb);
				rwcnt = matrowcnt(IINSNOb);
				for (y=rwcnt;y>=0;y=y-1) begin
					matrowget(IINSNOb,y-1,IINSNOrw);
					if(IINSNOrw.Category==INr.Category)then begin
						Catef = true;
						SNMiddleval = IINSNOrw.SNMiddle + 1;
						IINSNOrw.SNMiddle = IINSNOrw.SNMiddle + 1;
						IINSNOrw.Category = INr.Category;
						matrowput(IINSNOb,rwcnt,IINSNOrw);
						SNOneNumber = ValToString(SNMiddleval,M4val,"","",1);
						SNOneNumber = left(SNOneNumber,len(SNOneNumber)-2);
						for (j=0;j<5;j=j+1) begin
							strlen = len(SNOneNumber);
							if(strlen<5)then begin 
								SNOneNumber = "0" & SNOneNumber;
							end else begin
								j = 5;
							end;
						end;
						INr.SNOne = INr.Category & "-" & SNOneNumber;
						INr.SN = INr.SNOne & "-" & INr.CompletCode;
						IN2r.Code = INr.SN;
						while (ReadfirstMain(IN2r,1,true))begin
							SNMiddleval = IINSNOrw.SNMiddle + 1;
							IINSNOrw.SNMiddle = IINSNOrw.SNMiddle + 1;
							IINSNOrw.Category = INr.Category;
							matrowput(IINSNOb,rwcnt,IINSNOrw);
							SNOneNumber = ValToString(SNMiddleval,M4val,"","",1);
							SNOneNumber = left(SNOneNumber,len(SNOneNumber)-2);
							for (j=0;j<5;j=j+1) begin
								strlen = len(SNOneNumber);
								if(strlen<5)then begin 
									SNOneNumber = "0" & SNOneNumber;
								end else begin
									j = 5;
								end;
							end;
							INr.SNOne = INr.Category & "-" & SNOneNumber;
							INr.SN = INr.SNOne & "-" & INr.CompletCode;
							IN2r.Code = INr.SN;
						end;
						y=0;
					end;
				end;
				if (!Catef) then begin
					IINSNOrw.SNMiddle = 0;
					SNMiddleval = IINSNOrw.SNMiddle;
					IINSNOrw.Category = INr.Category;
					matrowput(IINSNOb,rwcnt,IINSNOrw);
					SNOneNumber = ValToString(SNMiddleval,M4val,"","",1);
					SNOneNumber = left(SNOneNumber,len(SNOneNumber)-2);
					for (j=0;j<5;j=j+1) begin
						strlen = len(SNOneNumber);
						if(strlen<5)then begin 
							SNOneNumber = "0" & SNOneNumber;
						end else begin
							j = 5;
						end;
					end;
					INr.SNOne = INr.Category & "-" & SNOneNumber;
					INr.SN = INr.SNOne & "-" & INr.CompletCode;
					IN2r.Code = INr.SN;
					while (ReadfirstMain(IN2r,1,true))begin
						SNMiddleval = IINSNOrw.SNMiddle + 1;
						IINSNOrw.SNMiddle = IINSNOrw.SNMiddle + 1;
						IINSNOrw.Category = INr.Category;
						matrowput(IINSNOb,rwcnt,IINSNOrw);
						SNOneNumber = ValToString(SNMiddleval,M4val,"","",1);
						SNOneNumber = left(SNOneNumber,len(SNOneNumber)-2);
						for (j=0;j<5;j=j+1) begin
							strlen = len(SNOneNumber);
							if(strlen<5)then begin 
								SNOneNumber = "0" & SNOneNumber;
							end else begin
								j = 5;
							end;
						end;
						INr.SNOne = INr.Category & "-" & SNOneNumber;
						INr.SN = INr.SNOne & "-" & INr.CompletCode;
						IN2r.Code = INr.SN;
					end;
				end;
				i = acnt;
			end;
		end;
    PutWindowRecord(wn,INr);        
  end;
  INDClassCategoryEFAfter = res;
  return;
end;


