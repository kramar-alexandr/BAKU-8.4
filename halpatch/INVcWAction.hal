global
procedure TransactionImportDsm()
begin
area afile;

string 255 fname;
xml fil;
string 20 obj;
array string 50 exp;
string 255 comment;
val sum,cursval;
boolean one;
integer i,cnt,cntdocs,j,cntrow,wn;
array string 50 curcode;
string 100 cursvalstring;
array string 100 acc;
record TRVc TRr;
row  TRVc TRrw,TR1rw,TR2rw;
Boolean testf,change;
 string 255 xstr,tstr;
cntdocs = 0;
if (OpenFileDialog("xml",fname)) then begin
	fil = ParseXMLFile(fname);
		curcode[0] = XmlGet(fil,"Data/payment[0]/curency");
		curcode[1] = XmlGet(fil,"Data/payment[0]/ExchangeCurency");
		obj = XmlGet(fil,"Data/payment[0]/store");
		sum = stringtoval(XmlGet(fil,"Data/payment[0]/sum"),M45Val);
		cursval = stringtoval(XmlGet(fil,"Data/payment[0]/KursVal"),M45Val);
		cursvalstring = XmlGet(fil,"Data/payment[0]/KursVal");
		TRr.TransDate = CurrentDate;
		TRrw.AccNumber = "51";
		TRrw.Objects = obj;
		if(LowerCase(curcode[0])!="") then begin
			TRrw.CurDebVal = sum;
			TRrw.DebVal = sum;
		end else begin 
			TRrw.CurCredVal = sum;
			TRrw.CredVal = sum;
		end;	
		
		matrowput(TRr,0,TRrw);
		TRVc_PasteObjects(TRr,0);
		TRVc_PasteAccNumber(TRr,1,0,true);
		matrowget(TRr,0,TRrw);
		TRrw.Curncy = "AZN";
		matrowput(TRr,0,TRrw);
		TRVc_PasteCurncy(TRr,1);
		matrowget(TRr,0,TRrw);
		TRrw.FrRate = 1;
		TRrw.ToRateB1 = 1;
		TRrw.ToRateB2 = 1;
		TRrw.BaseRate1 = 1;
		TRrw.BaseRate2 = 1;
		matrowput(TRr,0,TRrw);
		if(LowerCase(curcode[0])!="") then begin
			TRVc_PasteCurDebVal(TRr,0);
		end else begin 
			TRVc_PasteCurCredVal(TRr,0);
		end;	
		
		TR1rw.AccNumber = "52";
		TR1rw.Objects = obj;
		if(LowerCase(curcode[0])!="") then begin
			TR1rw.CurCredVal = Round(sum/cursval,SetRoundModeD(2));
		end else begin 
			TR1rw.CurDebVal = Round(sum/cursval,SetRoundModeD(2));
		end;	
		matrowput(TRr,1,TR1rw);
		TRVc_PasteObjects(TRr,1);
		TRVc_PasteAccNumber(TRr,1,1,true);
		matrowget(TRr,1,TR1rw);
		if(LowerCase(curcode[0])!="") then begin
			TR1rw.Curncy = curcode[0];
		end else begin 
			TR1rw.Curncy = curcode[1];
		end;	
		matrowput(TRr,1,TR1rw);	
		TRVc_PasteCurncy(TRr,1);
		matrowget(TRr,1,TR1rw)	;
		if(LowerCase(curcode[0])!="") then begin
			TRVc_PasteCurCredVal(TRr,1);
		end else begin 
			TRVc_PasteCurDebVal(TRr,1);
		end;	
		
		TR2rw.AccNumber = "84/03";
		TR2rw.Objects = obj;
		matrowput(TRr,2,TR2rw);	
		TRVc_PasteAccNumber(TRr,1,2,true);
		matrowget(TRr,2,TR2rw);
		TRVc_PasteObjects(TRr,2);
		TRDDefault(TRr,2,1);
		TRr.Comment = " " & Round(sum/cursval,SetRoundModeD(2)) & " " & TR1rw.Curncy  & "   " &  cursvalstring;
		wn = OpenWindow("TRDClass",1,0,"","",TRr);
  while (TestEOF==false) begin
    xstr = ImportField;
    testf = NextImportLine(true);     
  end;        
end;
	
return;
end;





