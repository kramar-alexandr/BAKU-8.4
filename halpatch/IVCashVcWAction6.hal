          case Rs_update:
            GetPrevWindowRecord(mwn,prevRestAccr);
            if (prevRestAccr.Closed!=0) then begin
              CloseWindow(wn);
              goto LProceedOpenOnHotelGuestTouchScreenDClass;
            end;
          case Rs_normal:
            GetWindowRecord(mwn,prevRestAccr);
            if (prevRestAccr.Closed!=0) then begin
              CloseWindow(wn);
              goto LProceedOpenOnHotelGuestTouchScreenDClass;
