  if (mwn!=0) then begin
    GetWindowRecord(wn,RepSpec);
    err = TestOpenOnHotelGuestTouchScreenOClass(RepSpec,errstr,fieldgoto);
    if (err!=0) then begin
      MessageBox(err," " & errstr);
      WindowFieldGoto(wn,RepSpec,-1,fieldgoto,true);
