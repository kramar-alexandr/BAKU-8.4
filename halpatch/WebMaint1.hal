		end;
	end;
	
	weboutstring("<table>");
	while(loopmain(CLOutr,1,true))begin
		if(setinset("AFF",CLOutr.Objects))then begin
			weboutstring("<tr>");
			weboutstring("<td>" & CLOutr.SerNr & "</td>");	
			weboutstring("<td>" & CLOutr.Objects & "</td>");	
			weboutstring("<td>" & CLOutr.Addr4 & "</td>");
			if(nonblank(CLOutr.Addr4))then begin
				if((CLOutr.Addr4=="AFF_TW" or CLOutr.Addr4=="AFF_CR") and !SetInSet("AFF_TW",CLOutr.Objects) and !SetInSet("AFF_CR",CLOutr.Objects))then begin
					CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF",CLOutr.Addr4);
					if(UpdateAFFTransFromCLOut(CLOutr))then begin
						recordstore(CLOutr,true);
					end;
				end;
			end else begin
				affobj = "";
				GetObjs("STORE",CLOutr.Objects,affobj);
				if (nonblank(affobj) and affobj!="CC" and !SetInSet("AFF_TW",CLOutr.Objects) and !SetInSet("AFF_CR",CLOutr.Objects)) then begin
					weboutstring("<tr>");
					weboutstring("<td>" & CLOutr.SerNr & "</td>");	
					weboutstring("<td>" & CLOutr.Objects & "</td>");	
					weboutstring("<td>" & CLOutr.Addr4 & "</td>");
					switch(affobj) begin 
						case "COINCASA": weboutstring("<td>" & "AFF_TW" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_TW");
						case "COINCASA2": weboutstring("<td>" & "AFF_TW" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_TW");
						case "VB1": weboutstring("<td>" & "AFF_TW" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_TW");
						case "VB2": weboutstring("<td>" & "AFF_TW" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_TW");
						case "SW3": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "SW5": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "SW6": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "SW7": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "EXTRAVAGANZA_2": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "EXTRAVAGANZA_3": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
						case "DENIZ_MALL": weboutstring("<td>" & "AFF_CR" & "</td>");
													CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF","AFF_CR");
					end;
					if(UpdateAFFTransFromCLOut(CLOutr))then begin
						recordstore(CLOutr,true);
					end;
					
					weboutstring("</tr>");
				end else begin
					if (SetInSet("CC",CLOutr.Objects) and SetInSet("AFF",CLOutr.Objects) and !SetInSet("AFF_TW",CLOutr.Objects) and !SetInSet("AFF_CR",CLOutr.Objects)) then begin
						matrowget(CLOutr,0,CLOutrw);
						affobj = "";
						GetObjs("FOB",CLOutrw.Objects,affobj);
						if (nonblank(affobj) and nonblank(vFobObj[affobj])) then begin
							CLOutr.Objects = StrReplace(CLOutr.Objects,"AFF",vFobObj[affobj]);
							if(UpdateAFFTransFromCLOut(CLOutr))then begin
								recordstore(CLOutr,true);
							end;
						end;
					end;
				end;
			end;
			weboutstring("</tr>");
		end;
		affobj = "";
		GetObjs("SUBDI",CLOutrw.Objects,affobj);
		if(blank(affobj))then begin
			if(nonblank(CLOutr.Addr4))then begin
				weboutstring("<tr>");
				weboutstring("<td>" & CLOutr.SerNr & "</td>");	
				weboutstring("<td>" & CLOutr.Objects & "</td>");	
				weboutstring("<td>" & CLOutr.Addr4 & "</td>");
				if((CLOutr.Addr4=="AFF_TW" or CLOutr.Addr4=="AFF_CR") and !SetInSet("AFF_TW",CLOutr.Objects) and !SetInSet("AFF_CR",CLOutr.Objects))then begin
					if(nonblank(CLOutr.Objects)) then begin CLOutr.Objects = CLOutr.Objects & ","; end;
					CLOutr.Objects = CLOutr.Objects & CLOutr.Addr4;
					if(UpdateAFFTransFromCLOut(CLOutr))then begin
						recordstore(CLOutr,true);
					end;
				end;
			end;
			weboutstring("</tr>");
		end;
	end;
	weboutstring("</table>");
	
return;
end;





