							end;
						end;
					end;
					if (setinset("ELG",TRrw.Objects)) then begin
						changed = true;
						TRrw.Objects = StrReplace(TRrw.Objects,"AFF","AFF_CR");
					end;
					if (setinset("SNT",TRrw.Objects)) then begin
						changed = true;
						TRrw.Objects = StrReplace(TRrw.Objects,"AFF","AFF_TW");
					end;
					if (changed) then begin
						matrowput(TRr,i,TRrw);
						TRchanget = true;
					end;
				end;
			end;
		end;
		if (TRchanget and change) then begin
			res = recordupdate(oldTRr,TRr,true)==0;
			if(res)then begin
				weboutstring("<td>" & "UPDATED" & "</td>");
			end else begin
				weboutstring("<td>" & "ERROR++++++++++++++++++++++++++++" & "</td>");
			end;
		end else begin
			res = true;
			weboutstring("<td>" & "GoodRecord" & "</td>");
		end;
		weboutstring("<tr>");
	end;
	weboutstring("</table>");

return;
end;




