global
webpublic procedure WebGetKareItCodesWithoutKINPO()
begin
	record ClientItemStatusVc CISr;
	record ConsItemVc CIr;
	record ORVc ORr;
	row ORVc ORrw;
	record POVc POr;
	row POVc POrw;
	longint i;
	vector boolean NoKItem;
	vector integer NoKItemRow;
	boolean TrHs, ORUPD;
 
	logtext(0,"================================================ WebGetKareItCodesWithoutK");
  SetCompany(18,false);
	
	weboutstring("<table>");
	ResetLoop(ORr);
	while (loopmain(ORr,1,true)) begin
		clearvector(NoKItem);
		clearvector(NoKItemRow);
		for (i=0;i<matrowcnt(ORr);i=i+1) begin
			matrowget(ORr,i,ORrw);
			if (left(ORrw.GlobalItemArtCode,1)!="K") then begin
				NoKItem[ORrw.GlobalItemArtCode] = true;
				NoKItemRow[ORrw.GlobalItemArtCode] = i;
			end;
		end;
		POr.OrdNr = ORr.SerNr;
		TrHs = true;
    while (loopkey("OrdNr",POr,1,TrHs)) begin
			if (POr.OrdNr!=ORr.SerNr) then begin TrHs = false; end;
			if (TrHs) then begin
				ORUPD = false;
				for (i=0;i<matrowcnt(POr);i=i+1) begin
					matrowget(POr,i,POrw);
					if (NoKItem[right(POrw.VEArtCode,len(POrw.VEArtCode)-1)] and left(POrw.VEArtCode,1)=="K") then begin
						weboutstring("<tr>");
						weboutstring("<td> POVc </td>");
						weboutstring("<td>" & POr.SerNr & "</td>");
						weboutstring("<td>" & ORr.SerNr & "</td>");
						weboutstring("<td>" & ORrw.GlobalItemArtCode & "</td>");
						weboutstring("<td>" & POrw.VEArtCode & "</td>");
						weboutstring("<td>" & i & "</td>");
						weboutstring("</tr>");
						matrowget(ORr,NoKItemRow[right(POrw.VEArtCode,len(POrw.VEArtCode)-1)],ORrw);
						if(ORrw.Shipd1==0)then begin
							ORrw.ArtCode = POrw.ArtCode;
							ORrw.GlobalItemArtCode = POrw.VEArtCode;
							matrowput(ORr,NoKItemRow[right(POrw.VEArtCode,len(POrw.VEArtCode)-1)],ORrw);
							ORUPD = true;
						end;
					end;
				end;
				if (ORUPD) then begin
					queued.UpdORVc(ORr);
					millisleep(50);
				end;
			end;
		end;
		ResetLoop(POr);
	end;
 return;
end;




