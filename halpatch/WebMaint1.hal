									if (apromo.length>0) then begin
										for(k=0;k<apromo.length;k=k+1)begin
											areb = apromo[k];
											if(nonblank(areb))then begin
												IVrw.Objects = RemoveObjectFromObjectList(IVrw.Objects,areb);
												changed = true;
											end;
										end;
										matrowput(IVr,i,IVrw);
										if (nonblank(newPromo)) then begin
											if (nonblank(IVrw.Objects)) then begin IVrw.Objects = IVrw.Objects & ","; end;
											IVrw.Objects = IVrw.Objects & newPromo;
											changed = true;
											matrowput(IVr,i,IVrw);
										end;
									end else begin
										if (nonblank(newPromo)) then begin
											if (nonblank(IVrw.Objects)) then begin IVrw.Objects = IVrw.Objects & ","; end;
											IVrw.Objects = IVrw.Objects & newPromo;
											changed = true;
											matrowput(IVr,i,IVrw);
										end;
									end;
								end else begin
									clearArray(apromo);
									oscnt = 0;
									ExtractObjectsByType(IVrw.Objects,"PROMO",apromo,oscnt);
									if (apromo.length>0) then begin
										for(k=0;k<apromo.length;k=k+1)begin
											areb = apromo[k];
											if(nonblank(areb))then begin
												IVrw.Objects = RemoveObjectFromObjectList(IVrw.Objects,areb);
												changed = true;
											end;
										end;
										matrowput(IVr,i,IVrw);
