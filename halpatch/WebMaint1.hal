global webpublic procedure WebDellBrandSectInGlItMn()
begin
	record GlobalItemVc GIr;
	record BtrxSecondLevelCatVc BtrxSecondLevelCatr, OldBtrxSecondLevelCatr;
	record BTRxBrandVc Brandr;
	boolean TrHs;
	string 255 WEBSSectStr, tmp;
	integer pos;
	
	logtext(0,"================================================ WebDellBrandSectInGlItMn");
	GIr.Code = "";
	while (loopMain(GIr,1,true)) begin
		if (nonblank(GIr.BPIBrand) and nonblank(GIr.BTRxGiftClass)) then begin
			TrHs = false;
			WEBSSectStr = "";
			pos = 0;
			ExtractObj(GIr.BTRxGiftClass,pos,tmp);
			while (nonblank(tmp)) begin
				OldBtrxSecondLevelCatr.Code = tmp;
				if (!ReadFirstMain(OldBtrxSecondLevelCatr,1,true)) then begin
					if (nonblank(WEBSSectStr)) then begin WEBSSectStr = WEBSSectStr & ","; end;
					WEBSSectStr = WEBSSectStr & tmp;
				end;
				ExtractObj(GIr.BTRxGiftClass,pos,tmp);
			end;
			GIr.BTRxGiftClass = WEBSSectStr;
			queued.SaveGIWithoutBrandSect(GIr);
			MilliSleep(50);
		end;
	end;
	
	
	BtrxSecondLevelCatr.Code = "";
	while (loopMain(BtrxSecondLevelCatr,1,true))  begin
		if (BtrxSecondLevelCatr.PathFrstCode=="BRANDS") then begin
			queued.DellBrandSect(BtrxSecondLevelCatr);
			MilliSleep(40);
		end;
	end;
	
	
return;
end;

global webpublic procedure WebReportCheckGIRn()
begin
	record GlobalItemVc GIr,mainGIr;
	row GlobalItemVc GIrw;
	record ItemStatusVc ISr;
	boolean TrHs;
	integer i;
	
	SetCompany(1,false);

	logtext(0,"================================================ WebReportCheckGIRn");
	TrHs = true;
	GIr.Code = "BRND0171";
	while(loopmain(GIr,1,TrHs))begin
		if(left(GIr.Code,8)!="BRND0171")then begin TrHs = false; end;
		for(i=0;i<matrowcnt(GIr);i=i+1) begin
			matrowget(GIr,i,GIrw);
			ISr.Location = GIrw.Location;
			if(ReadFirstKey("Location",ISr,1,true))then begin
				if(GIrw.Instock!=ISr.Instock - ISr.RsrvQty)then begin
					weboutstring(GIr.HansaCode & " / " & GIrw.Instock & " / " & ISr.Instock - ISr.RsrvQty & "<br>");
				end;
			end;
		end;
	end;
	
	return;
end;




