global webpublic updating procedure WebDelEndResActsMn()
begin
	record ActVc Actr, OldActr;
	integer i;
	boolean TrHs, testf;
	
	logtext(0,"================================================ WebDelEndResActsMn");
	for (i=0;i<34;i=i+1) begin
		SetCompany(i,false);
		Actr.OKFlag = 0;
		TrHs = true;
		While(loopkey("OKFlag",Actr,1,TrHs)) begin
			testf = true;
			if(Actr.OKFlag>0)then begin TrHs = false; testf = false; end;	  
			if (left(Actr.Comment,28) != "Истекает срок резервирования") then begin testf = false; end;
			if(testf)then begin
				RecordCopy(OldActr,Actr);
				Actr.OKFlag = 1;
				RecordUpdate(OldActr,Actr,true);
				logtext (0,Actr.SerNr & "   " & CurrentCompany);
			end;
		end;
		Resetloop(Actr);
	end;
	
	return;
end;




global webpublic procedure WebShowAllNotSHECItemsRn()
begin
	record ORVc ORr;
	row ORVc ORrw;
	record SHVc SHr;
	row SHVc SHrw;
	record IVVc IVr;
	row IVVc IVrw;
	integer i, j;
	boolean TrHs, found;
	vector longint ItemSHNr,ItemIVNr;
	
	logtext(0,"================================================ WebDelEndResActsMn");
	for (i=1;i<=34;i=i+1) begin
		SetCompany(i,false);
		ORr.SerNr = -1;
		while (loopMain(ORr,1,true)) begin
			if (ORr.ECOMMERCEOrdf==1) then begin
				ClearVector(ItemSHNr);
				ClearVector(ItemIVNr);
				SHr.OrderNr = ORr.SerNr;
				found = true;
				while (LoopKey("OrderKey",SHr,1,found)) begin
					if (SHr.OrderNr!=ORr.SerNr) then begin found = false; end;
					if (found) then begin
						for (j=0;j<matrowcnt(SHr);j=j+1) begin
							matrowget (SHr,j,SHrw);
							ItemSHNr[SHrw.ArtCode] = SHr.SerNr;
						end;
					end;
				end;
				ResetLoop(SHr);
				IVr.OrderNr = ORr.SerNr;
				found = true;
				while (LoopKey("OrderKey",IVr,1,found)) begin
					if (IVr.OrderNr!=ORr.SerNr) then begin found = false; end;
					if (found) then begin
						for (j=0;j<matrowcnt(IVr);j=j+1) begin
							matrowget (IVr,j,IVrw);
							ItemIVNr[IVrw.ArtCode] = IVr.SerNr;
						end;
					end;
				end;
				ResetLoop(IVr);
				for (j=0;j<matrowcnt(ORr);j=j+1) begin
					matrowget (ORr,j,ORrw);
					if (ORrw.Quant>ORrw.Shipd2) then begin
						weboutstring("<tr>");
							weboutstring("<td>" & ORr.OrdDate & "</td>");
							weboutstring("<td>" & CurrentCompany & "</td>");
							weboutstring("<td>" & ItemIVNr[ORrw.ArtCode] & "</td>");
							weboutstring("<td>" & ORrw.ArtCode & "</td>");
							weboutstring("<td>" & ItemSHNr[ORrw.ArtCode] & "</td>");
							weboutstring("<td>" & ORrw.Shipd2 & "/" & ORrw.Quant & "</td>");
							weboutstring("<td>" & ORrw.Quant - ORrw.Shipd2 & "</td>");
						weboutstring("</tr>");
					end;
				end;
			end;
		end;
		Resetloop(ORr);
	end;
	
	return;
end;







