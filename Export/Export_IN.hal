//server-only
external procedure ExtractObj(string,var Integer,var string);

SetLangMode(LangRussian,"RUS",0);

Global procedure ExportINEn(record RcVc RepSpec)
begin
	record INVc INr;
	string 100 tstr;
	record DIVc DIr;
	integer i;
	boolean testf;
	
	if(nonblank(RepSpec.f1))then begin
		INr.DispGroups = RepSpec.f1;
		testf = true;
		while(loopkey("MyDispGroups",INr,1,testf))begin
			if(	left(INr.DispGroups,len(RepSpec.f1))!=RepSpec.f1)then begin testf = false; end;
			
			
			if(testf)then begin
				exportstring(INr.Code);
				exportstring(INr.AlternativeCode);
				exportstring(INr.Name);
			
				tstr = "";
				i=0;
				ExtractObj(INr.DispGroups,i,tstr);
				DIr.Code = tstr;
				if(readfirstmain(DIr,1,true))then begin
					tstr = Dir.Name;
				end else begin
					tstr = "";
				end;
				exportstring(tstr);
			
				ExtractObj(INr.DispGroups,i,tstr);
				DIr.Code = tstr;
				if(readfirstmain(DIr,1,true))then begin
					tstr = Dir.Name;
				end else begin
					tstr = "";
				end;
				exportstring(tstr);
			
				ExtractObj(INr.DispGroups,i,tstr);
				DIr.Code = tstr;
				if(readfirstmain(DIr,1,true))then begin
					tstr = Dir.Name;
				end else begin
					tstr = "";
				end;
				exportstring(tstr);
				NewLine;
			end;
		end;
	end else begin
		INr.Code = "";
		while(loopmain(INr,1,true))begin
			exportstring(INr.Code);
			exportstring(INr.AlternativeCode);
			exportstring(INr.Name);
			
			tstr = "";
			i=0;
			ExtractObj(INr.DispGroups,i,tstr);
			DIr.Code = tstr;
			if(readfirstmain(DIr,1,true))then begin
				tstr = Dir.Name;
			end else begin
				tstr = "";
			end;
			exportstring(tstr);
			
			ExtractObj(INr.DispGroups,i,tstr);
			DIr.Code = tstr;
			if(readfirstmain(DIr,1,true))then begin
				tstr = Dir.Name;
			end else begin
				tstr = "";
			end;
			exportstring(tstr);
			
			ExtractObj(INr.DispGroups,i,tstr);
			DIr.Code = tstr;
			if(readfirstmain(DIr,1,true))then begin
				tstr = Dir.Name;
			end else begin
				tstr = "";
			end;
			exportstring(tstr);
			
			NewLine;
		end;
	end;
	
	
 return;
end;


